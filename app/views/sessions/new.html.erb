<h2>Login</h2>
<%= form_for :session, url: login_path do |f| %>
  <%= f.label :username %>
  <%= f.text_field :username%>
  <%= f.label :password %>
  <%= f.password_field :password %>
  <%= f.submit "Login" %>
<% end %>

<p>
  Don't have an account yet? <%= link_to "Sign up!", new_user_path%>
</p>
