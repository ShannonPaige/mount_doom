<div class="entire center">
  <h2>Create an Account</h2>
  <div class="container">
  <div class="form">
    <%= form_for @user do |f| %>
      <%= f.label :username, class: "label" %>
      <%= f.text_field :username %>
      <br>
      <%= f.label :password, class: "label" %>
      <%= f.password_field :password %>
      <%= f.submit "Create Account", class: "btn", style: "margin-left: -20px" %>
      <% end %>
    </div>
  </div>
</div>
