<%= form_for @chip, url: admin_chip_path do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name%>
  <%= f.label :price %>
  <%= f.text_field :price%>
  <%= f.label :description %>
  <%= f.text_field :description%>
  <%= f.label :image %>
  <%= f.text_field :image%>
  <%= f.submit 'Update Chip'%>
<% end %>
