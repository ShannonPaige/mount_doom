<%= form_for @chip, url: admin_chip_path do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name%>
  <%= f.label :price %>
  <%= f.text_field :price%>
  <%= f.label :description %>
  <%= f.text_field :description%>
  <%= f.label :status %>
  <%= f.text_field :status%>
  <%= f.label :image %>
  <%= f.file_field :image%><br><br>
  <%= f.submit 'Update Chip', class: 'btn' %>
<% end %>
